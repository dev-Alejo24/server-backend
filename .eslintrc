{
    "extends": [
      "airbnb-base",
      "airbnb-typescript/base"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "import/prefer-default-export": 0,
      "class-methods-use-this": 0,
      "import/no-extraneous-dependencies": 0,
      "no-param-reassign": 0,
      "no-throw-literal": 0,
      "@typescript-eslint/no-throw-literal": 0,
      "no-restricted-syntax": 0,
      "max-len": "off"
    }
  }